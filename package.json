{
  "name": "@left0ver/eslint-config",
  "version": "0.0.3",
  "private": true,
  "description": "eslint config repo",
  "author": "left0ver",
  "license": "MIT",
  "homepage": "https://github.com/left0ver/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/left0ver/eslint-config.git"
  },
